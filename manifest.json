{
    "manifest_version":2,
    "name":"Notes Launcher",
    "description":"Quick launch your notes and pdfs.",
    "version":"1.0.0",
    "icons":{"128":"./src/images/icon_128.png"},
    "browser_action":{
        "default_icon":"./src/images/icon.png",
        "default_popup":"./src/popup.html"
    },
    "permissions":["activeTab"],
    "background": {
    "page": "./src/popup.html"
    }
}